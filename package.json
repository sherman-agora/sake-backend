{
  "name": "petgo-services",
  "private": true,
  "resolutions": {
    "graphql": "^14.6.0"
  },
  "workspaces": [
    "services/*"
  ],
  "scripts": {
    "build:all": "docker-compose --env-file services/.env -f services/docker-compose.yml build",
    "build:prod": "docker-compose --env-file services/.env -f services/docker-compose.production.yml build",
    "push:prod": "docker-compose --env-file services/.env -f services/docker-compose.production.yml push",
    "start:base": "docker-compose --env-file services/.env -f services/docker-compose.base.yml up -d",
    "start:all": "docker-compose --env-file services/.env -f services/docker-compose.yml up -d",
    "stop:base": "docker-compose --env-file services/.env -f services/docker-compose.base.yml down",
    "stop:all": "docker-compose --env-file services/.env -f services/docker-compose.base.yml down --remove-orphans",
    "lerna:add": "lerna add ",
    "lerna": "lerna"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "lerna": "^3.4.3",
    "prettier": "^1.19.1"
  },
