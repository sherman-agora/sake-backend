    "@apollo/gateway": "^0.10.8",
    "apollo-boost": "^0.4.7",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.4",
    "apollo-link-http": "^1.5.16",
    "apollo-server": "^2.11.0",
    "apollo-server-testing": "^2.9.12",
    "axios": "^0.19.2",
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "bcryptjs": "2.4.3",
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "cross-fetch": "^3.0.4",
    "dotenv": "^8.2.0",
    "express": "^4.13.3",
    "faker": "^4.1.0",
